    <script>    

        async function obtenerDatos() {
            try 
            {
                const respuesta = await fetch('https://api.thedogapi.com/v1/breeds');
                const datos = await respuesta.json();
                console.log(datos);
            } 
            catch (error) {
                console.error(error);   
            }
        }

        obtenerDatos();


    </script>

